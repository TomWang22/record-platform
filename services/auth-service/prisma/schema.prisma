datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL_AUTH") // ?schema=auth
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
