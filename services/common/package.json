{
  "name": "@common/utils",
  "version": "0.3.1",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./logger": "./dist/logger.js",
    "./metrics": "./dist/metrics.js",
    "./auth": "./dist/auth.js",
    "./kafka": "./dist/kafka.js",
    "./redis": "./dist/redis.js",
    "./grpc-clients": "./dist/grpc-clients.js",
    "./proto": "./dist/proto.js"
